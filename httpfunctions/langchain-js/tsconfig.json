{
  "compilerOptions": {
    "rootDir": "./fakesrc",
    "outDir": "./src",
    "module": "nodenext",
    "target": "esnext",
    "types": [],
    "sourceMap": false,
    "declaration": false,
    "declarationMap": false,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "strict": true,
    "jsx": "react-jsx",
    "isolatedModules": true,
    "noUncheckedSideEffectImports": true,
    "moduleDetection": "force",
    "skipLibCheck": true
  }
}

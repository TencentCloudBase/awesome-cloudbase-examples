    "@cloudbase/functions-typings": "^1.0.0",
    "@cloudbase/node-sdk": "^3.7.0",
    "openai": "^4.77.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudbase/functions-framework": "^1.6.0",
    "@dotenvx/dotenvx": "^1.38.4",
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
